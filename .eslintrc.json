{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
       "object-curly-spacing": "off",
       "new-cap": "off",
       "require-jsdoc": "off",
       "comma-dangle": "off",
       "padded-blocks": "off",
       "max-len": "off",
       "no-multiple-empty-lines": ["error", { "max": 3, "maxEOF": 0 }],
       "indent": "off",
       "array-bracket-spacing": "off",
       "quotes": "off",
       "semi": "off",
       "operator-linebreak": "off"
    }
}
